git_url("git@bitbucket.org:teamnursa/nursa-certificates.git")

storage_mode("git")

type("adhoc") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.nikorozo.ionic.test"])
username "appdev@nursa.com" # Your Apple Developer Portal username

# For all available options run `fastlane match --help`
# Remove the # in the beginning of the line to enable the other options

# Command used to generate the certificates and profiles:
# bundle exec fastlane match adhoc
# bundle exec fastlane match development
# bundle exec fastlane match appstore

# The docs are available on https://docs.fastlane.tools/actions/match
